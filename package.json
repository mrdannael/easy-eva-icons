{
  "name": "easy-eva-icons",
  "version": "0.1.0",
  "description": "Easy to use eva icons.",
  "keywords": [
    "eva-icons",
    "icons",
    "react"
  ],
  "repository": "git@github.com:mrdannael/easy-eva-icons.git",
  "author": "mrdannael",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepublishOnly": "npm run optimize && npm run build",
    "build": "npm run build:react && npm-run-all --parallel build:esm build:cjs",
    "build:react": "node ./scripts/build react",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir ./lib/react/cjs",
    "postbuild": "npm run copy:react",
    "optimize": "rimraf ./svg && npm-run-all --parallel optimize:fill optimize:outline",
    "optimize:fill": "svgo -q -f ./src/fill -o ./svg",
    "optimize:outline": "svgo -q -f ./src/outline -o ./svg",
    "copy:react": "cp -R ./src/packages/react.json ./lib/react/package.json"
  },
  "devDependencies": {
    "@svgr/core": "^5.5.0",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.0",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "svgo": "^2.3.0",
    "typescript": "^4.3.2"
  }
}
